{
  "name": "Extrahand",
  "version": "0.0.1",
  "private": true,
  "displayName": "Extrahand",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "webpack serve --config webpack.config.js --mode development",
    "web:dev": "NODE_ENV=development webpack serve --config webpack.config.js --mode development",
    "web:prod": "NODE_ENV=production webpack serve --config webpack.config.js --mode production",
    "build:web": "NODE_ENV=production webpack --config webpack.prod.simple.js --mode production",
    "build:web:analyze": "NODE_ENV=production webpack --config webpack.config.js --mode production --analyze",
    "build:docker": "docker build -t extrahand .",
    "build:docker:simple": "docker build --file Dockerfile.simple -t extrahand:latest .",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "deploy:fixed": "chmod +x deploy-fixed.sh && ./deploy-fixed.sh",
    "quick-fix": "chmod +x quick-fix.sh && ./quick-fix.sh",
    "deploy:caprover": "npm run build:web && git add . && git commit -m 'Deploy to CapRover' && git push",
    "deploy:staging": "npm run build:web:staging && git add . && git commit -m 'Deploy to Staging' && git push",
    "icons:info": "node generate-icons.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "clean": "rm -rf dist node_modules/.cache",
    "prebuild": "npm run clean && npm run lint && npm run type-check",
    "postbuild": "npm run security:audit"
  },
  "dependencies": {
    "@react-native-community/geolocation": "^3.4.0",
    "@react-native-firebase/app": "^23.0.1",
    "@react-native-firebase/auth": "^23.0.1",
    "@react-native/new-app-screen": "0.80.1",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.4",
    "@types/leaflet": "^1.9.20",
    "dotenv": "^17.2.1",
    "firebase": "^12.1.0",
    "leaflet": "^1.9.4",
    "lucide-react-native": "^0.525.0",
    "react": "19.1.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "react-native": "0.80.1",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.27.2",
    "react-native-maps": "^1.24.10",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-svg": "^15.12.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.20.0",
    "webpack-dev-server": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-flow": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "latest",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "0.80.1",
    "@react-native/metro-config": "0.80.1",
    "@react-native/typescript-config": "0.80.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "@webpack-cli/serve": "^3.0.1",
    "babel-loader": "^10.0.0",
    "buffer": "^6.0.3",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^12.0.2",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^7.1.2",
    "eslint": "^8.19.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.6.3",
    "mini-css-extract-plugin": "^2.8.1",
    "path-browserify": "^1.0.1",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "stream-browserify": "^3.0.0",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.10",
    "typescript": "5.0.4",
    "webpack": "^5.100.2",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^6.0.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@react-native"
  },
  "engines": {
    "node": ">=18"
  }
}
